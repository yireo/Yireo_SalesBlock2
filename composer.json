{
  "name": "yireo/magento2-salesblock2",
  "license": "OSL-3.0",
  "type": "magento2-module",
  "version": "2.2.0",
  "homepage": "https://www.yireo.com/software/magento-extensions/salesblock2",
  "description": "Block transactions based on various rules",
